int i=5,j=4,k=3,l=2,m=1;
main(i)
{
	int i=20,j=i+2;
	print(i!=j);
	print(i<=j);
	print(i);
	print("\n");
	k=4*i--3;
	l=i<=10;
	m=i/2;
	print("i=",i,"\n"); 
	print("j=",j,"\n"); 
	print("k=",k,"\n"); 
	print("l=",l,"\n"); 
	print("m=",m,"\n"); 
	print("func=",func(3*5,100/2),"\n");
}
func(x,y)
{
	int ret=0;
	for (;ret<x*y;) {
		ret=ret+y;
	}
	return ret;
}