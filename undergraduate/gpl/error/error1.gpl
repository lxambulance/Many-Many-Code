int i=5,j=4,k=3,l=2,0m=1;
main()
{
	int i=20,j=i+2;
	print(i!=j);
	print(i<=j);
	print(i);
	print("\n");
	k=i-3;
	l=i*2;
	m=i/2;
	print("i=",i,"\n"); 
	print("j=",j,"\n"); 
