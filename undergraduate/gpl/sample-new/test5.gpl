int i=3*5*7*39223;
main()
{
	int j;
	for (j=2;j*j<i;j=j+1) {
		if (i==i/j*j) {
			print("x=",j);
			print("func(",j,")=",func(j),"\n");
			while (i==i/j*j) {
				i=i/j;
			}
		}
	}
	if (i>1) {
		print("x=",i);
		print("func(",i,")=",func(i),"\n");
	}
}
func(x)
{
	int i=1,ret=0;
	while (i<=x) {
		ret=ret+i;
		i=i+1;
	}
	return ret;
}